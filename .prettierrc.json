{
  "arrowParens": "avoid",
  "proseWrap": "always",
  "semi": false,
  "singleQuote": false,
  "trailingComma": "all",
  "pluginSearchDirs": [],
  "plugins": [
    "prettier-plugin-packagejson",
    "prettier-plugin-organize-imports",
    "prettier-plugin-astro",
    "@prettier/plugin-xml",
    "prettier-plugin-tailwindcss"
  ],
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "parser": "astro"
      }
    }
  ]
}
