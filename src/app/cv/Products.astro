---
import { md2html } from "../markdown"

type Products = ReadonlyArray<{ name: string; url: string }>

interface Props {
  products: Products
}

const { products } = Astro.props

const toMD = (products: Products): string =>
  `Worked on ${products.map(x => `[${x.name}](${x.url})`).join(", ")}`
---

<Fragment set:html={md2html(toMD(products))} />
