---
import Root from "./HtmlRoot.astro"

interface Props {
  frontmatter: Partial<Record<string, string>>
}

const {
  frontmatter: { title, description },
} = Astro.props
---

<Root title={title} description={description}>
  <main class="prose m-6 max-w-prose sm:m-12 md:m-16">
    <slot />
  </main>
</Root>
